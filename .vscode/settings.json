{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "cSpell.words": [
    "bumpp",
    "caniuse",
    "colours",
    "commitlint",
    "composables",
    "Docsearch",
    "esbuild",
    "gsap",
    "iarna",
    "nprogress",
    "pnpm",
    "portfinder",
    "Tongji",
    "tsbuildinfo",
    "vite",
    "vuepress",
    "vueuse"
  ]
}
